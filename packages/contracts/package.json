{
  "name": "@workspace/contracts",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "build": "tsp compile ./spec && pnpm generate",
    "build:tsp": "tsp compile ./spec",
    "generate": "openapi-ts",
    "watch": "tsp compile ./spec --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.80.7",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.89.1",
    "@typespec/compiler": "^1.7.1",
    "@typespec/http": "^1.7.0",
    "@typespec/openapi3": "^1.7.0",
    "@typespec/rest": "^0.77.0",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client/index.ts",
    "./zod": "./src/zod.gen.ts",
    "./sdk": "./src/sdk.gen.ts",
    "./query": "./src/@tanstack/react-query.gen.ts",
    "./openapi": "./tsp-output/openapi/openapi.yaml",
    "./openapi.json": "./tsp-output/openapi/openapi.json"
  }
}
